<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.wat.animal_hospital.android_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Android_app">
        <activity android:name=".ui.client.VisitNotComplete"></activity>
        <activity android:name=".ui.doctor.VisitComplete_Doctor" />
        <activity android:name=".ui.VisitComplete" />
        <activity android:name=".ui.Visit_Not_Complete" />
        <activity android:name=".ui.doctor.CreateVisit_Doctor" />
        <activity android:name=".ui.client.CreateVisit_Client" />
        <activity android:name=".ui.Visit_Complete" />
        <activity android:name=".ui.doctor.ChooseClient" />
        <activity android:name=".ui.client.ChooseDoctor" />
        <activity android:name=".ui.doctor.DoctorDashboard" />
        <activity android:name=".ui.client.ClientDashboard" />
        <activity android:name=".ui.Login" />
        <activity android:name=".ui.Register" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>